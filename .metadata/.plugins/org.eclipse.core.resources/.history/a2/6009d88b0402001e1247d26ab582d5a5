package BaiNhom;

public class Calculator {
	 public double calculateTotalPrice(double[] prices) {
	        double total = 0;
	        for (int i = 0; i < prices.length; i++) {
	            total += prices[i];
	        }
	        if (total > 100) {
	            return total * 0.9;
	        } else {
	            return total;
	        }
	    }
}
